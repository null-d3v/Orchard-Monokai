@using Orchard.Mvc.Html;
@using Orchard.UI.Resources;
@{
    RegisterLink(new LinkEntry
    {
        Type = "image/png",
        Rel = "icon",
        Href = Url.Content(
            "~/Themes/null-d3v/Images/192.png"),
    });

    RegisterLink(new LinkEntry
    {
        Type = "image/png",
        Rel = "apple-touch-icon",
        Href = Url.Content(
            "~/Themes/null-d3v/Images/180.png"),
    });

    RegisterLink(new LinkEntry
    {
        Type = "image/x-icon",
        Rel = "shortcut icon",
        Href = Url.Content(
            "~/Themes/null-d3v/Images/32.ico"),
    });

    var siteName = Convert.ToString(
        WorkContext.CurrentSite.SiteName);
    var ogImage =
        "https://www.null-d3v.gg/Themes/null-d3v/Images/og.png";
}

<!DOCTYPE html> 
<html class="static @Html.ClassForPage()" lang="@WorkContext.CurrentCulture"> 
    <head> 
        <meta charset="utf-8"/>
        <meta content="width=device-width, user-scalable=no" name="viewport">
        <meta name="mobile-web-app-capable" content="yes" />
        <meta name="theme-color" content="#171714" />
        <meta property="fb:app_id" content="966242223397117">
        <meta property="og:description" content="I like to drink beer when I'm playing video games and when I'm not playing video games.">
        <meta property="og:image" content="@ogImage">
        <meta property="og:image:height" content="200">
        <meta property="og:image:width" content="584">
        <meta property="og:title" content="@siteName">
        <title>@Html.Title(siteName)</title> 
        @Display(Model.Head)
    </head> 
    <body>
        @Display(Model.Body)
        @Display(Model.Tail)
    </body>
</html>